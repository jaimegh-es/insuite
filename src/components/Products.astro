<section class="products" id="editores">
  <div class="container">
    <div class="section-header">
      <h2 class="section-title">Suite completa de productividad</h2>
      <p class="section-description">
        Aplicaciones diseñadas para su día a día
      </p>
    </div>

    <div class="products-grid">


      <!--InDOC -->
      <div class="product-card writer-card">
        <div class="product-visual">
            <img src="https://hosted.inled.es/INDOC.png" class="productos-imagen">
        </div>
        <div class="product-content">
          <h3 class="product-title">InDoc</h3>
          <p class="product-subtitle">Editor de texto en línea</p>
          <ul class="product-features">
            <li>Integración de Inled AI </li>
            <li>Exportación a PDF y texto</li>
            <li>Formateo de texto</li>
            <li>Continuar por donde lo dejaste</li>
          </ul>
          <a href="/indoc" class="btn btn-secondary">Explorar InDOC</a>
        </div>
      </div>

      <!--InMD -->

      <div class="product-card writer-card">
        <div class="product-visual">
            <img src="https://hosted.inled.es/inMD.png" class="productos-imagen">
        </div>
        <div class="product-content">
          <h3 class="product-title">InMD</h3>
          <p class="product-subtitle">El único editor visual de Markdown libre</p>
          <ul class="product-features">
            <li>Integración de Inled AI </li>
            <li>Exportación a MD y HTML</li>
            <li>Edición sin código</li>
            <li>Visualización de código sincronizada</li>
          </ul>
          <a href="/inmd" class="btn btn-secondary">Explorar InMD</a>
        </div>
      </div>

      <!--InLinked -->
      <div class="product-card writer-card">
        <div class="product-visual">
            <img src="https://hosted.inled.es/INLINKED.png" class="productos-imagen">
        </div>
        <div class="product-content">
          <h3 class="product-title">InLinked</h3>
          <p class="product-subtitle">Formateador de Post de LinkedIN</p>
          <ul class="product-features">
            <li>Integración de Inled AI </li>
            <li>Compatible con el formato LinkedIN</li>
          </ul>
          <a href="/inlinked" class="btn btn-secondary">Explorar InLinked</a>
        </div>
      </div>


      <!--MDPDF -->
      <div class="product-card writer-card">
        <div class="product-visual">
            <img src="https://hosted.inled.es/MDPDF.png" class="productos-imagen">
        </div>
        <div class="product-content">
          <h3 class="product-title">MDPDF</h3>
          <p class="product-subtitle">Edita y visualiza markdown y exporta a PDF</p>
          <ul class="product-features">
            <li>Compatibilidad con fórmulas LaTeX</li>
            <li>Compatible con el formato de ChatGPT, Deepseek, Gemini...</li>
            <li>Compatible con emoticonos</li>
            <li>Exportación a PDF con formato idéntico</li>
          </ul>
          <a href="/mdpdf" class="btn btn-secondary">Explorar MDPDF</a>
        </div>
      </div>

      <!--InQR -->
      <div class="product-card writer-card">
        <div class="product-visual">
            <img src="https://hosted.inled.es/inqr.png" class="productos-imagen">
        </div>
        <div class="product-content">
          <h3 class="product-title">InQR</h3>
          <p class="product-subtitle">Generador de códigos QR</p>
          <ul class="product-features">
            <li>Genera QR de un sitio web</li>
            <li>Crea QR de acceso a Wifi</li>
            <li>Compatible con múltiples tipos de Wifi</li>
            <li>Compone códigos de barras</li>
          </ul>
          <a href="/inqr" class="btn btn-secondary">Explorar InQR</a>
        </div>
      </div>

      <!--InQR -->
      <div class="product-card writer-card">
        <div class="product-visual">
            <img src="https://hosted.inled.es/inpdf.png" class="productos-imagen">
        </div>
        <div class="product-content">
          <h3 class="product-title">InPDF</h3>
          <p class="product-subtitle">Visor de PDF con IA</p>
          <ul class="product-features">
            <li>Más funciones que el visor predeterminado de Chrome</li>
            <li>Máxima privacidad</li>
            <li>Inteligencia artificial avanzada</li>
          </ul>
          <a href="https://inpdf.inled.es" class="btn btn-secondary">Explorar InPDF</a>
        </div>
      </div>
      
    </div>
  </div>
</section>

